<h1>Listing question_sets</h1>

<table>
  <tr>
    <th>ID</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @question_sets.each do |question_set| %>
  <tr>
    <td><%= question_set.id %> </td>
    <td><%= link_to 'Show', question_set %></td>
    <td><%= link_to 'Edit', edit_question_set_path(question_set) %></td>
    <td><%= link_to 'Destroy', question_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Question set', new_question_set_path %>
